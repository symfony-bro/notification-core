{
    "name" : "symfony-bro/notification-core",
    "description" : "Schedule bundle",
    "type" : "library",
    "authors" : [{
        "name" : "Artem Dekhtyar",
        "email" : "m@artemd.ru"
    },{
        "name" : "Pavel Stepanets",
        "email" : "pahhan.ne@gmail.com"
    }],
    "keywords" : [
        "notification"
    ],
    "require" : {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "google/apiclient": "^2.0"
    },
    "autoload" : {
        "psr-4" : {
            "SymfonyBro\\NotificationCore\\" : "src/",
            "SymfonyBro\\NotificationCore\\Tests\\" : "tests/"
        }
    },
    "suggest": {
        "swiftmailer/swiftmailer ": "Allows using swiftmailer driver",
        "ext-curl": "Allows using telegram and slack driver",
        "guzzlehttp/guzzle": "Allows to use guzzle driver",
        "google/apiclient": "For firebase cloud messaging"
    }
}
